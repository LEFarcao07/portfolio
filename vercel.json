{
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1.html",
      "headers": {
        "Content-Type": "text/html; charset=utf-8"
      }
    },
    {
      "src": "/404",
      "dest": "/404.html",
      "headers": {
        "Content-Type": "text/html; charset=utf-8"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}