{
  "version": 2,
  "builds": [
    {
      "src": "offline/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/offline/service-worker.js",
      "headers": {
        "Service-Worker-Allowed": "/",
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/offline/(.*)",
      "dest": "/offline/$1"
    },
    {
      "src": "/",
      "dest": "/offline/index.html"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/offline/offline.html"
    }
  ]
}